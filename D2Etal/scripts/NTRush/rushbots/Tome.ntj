function NTMain() {
	Include("Common/Libs/CLCommon.ntl");
	Include("NTRush/funcs/RushCommon.ntl");
	Common.IncludeFuncs();
	CLC_IncludeLibs();
	Config.Load();

	NT_LoadConfig();
	CLSI_LoadNIPFiles("Configs/"+me.realm+"/item_configs");

	CLA_Initialize();

	if (!Extras.Tome()) {
		Common.LogError("Tome()");
		Script.SendMsg("OverWatch.ntj", "Tome()");
		return;
	}

	Script.SendMsg("OverWatch.ntj", "SCRIPT_END"); 
}